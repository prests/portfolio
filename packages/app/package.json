{
  "name": "shayne-portfolio-frontend",
  "author": {
    "name": "Shayne Preston",
    "email": "shayne.preston@protonmail.com",
    "url": "https://www.shaynepreston.com/"
  },
  "description": "My portfolio's frontend",
  "homepage": "https://www.shaynepreston.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:prests/shayne-website-frontend.git"
  },
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean:build": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "dev": "vite --port 8080",
    "build": "pnpm run clean:build && vite build",
    "lint": "eslint --ext .ts,.vue --ignore-path ../../.gitignore src e2e && vue-tsc --noEmit",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ci": "playwright test --quiet",
    "preview": "vite preview --port 8080"
  },
  "dependencies": {
    "motion": "^10.14.2",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@types/node": "^18.8.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "eslint": "^8.18.0",
    "eslint-config-shayne": "latest",
    "jsdom": "^21.1.1",
    "path": "^0.12.7",
    "sass": "^1.52.3",
    "shayne-tsconfig": "latest",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-eslint": "^1.6.1",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.0.0"
  },
  "license": "UNLICENSED"
}
